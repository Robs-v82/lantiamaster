				<% end %>
				<% if query.con_senales_de_tortura %>
					<i class="tiny material-icons">check</i> Con se침ales de tortura<br> 
				<% end %>
				<% if query.crucificado %>
					<i class="tiny material-icons">check</i> Crucificado<br> 
				<% end %>
				<% if query.decapitado_cabeza_sin_cuerpo %>
